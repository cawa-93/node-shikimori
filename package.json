    "axios": "^0.18.0",
    "request": "^2.79.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-config-standard": "^8.0.1"
  },
  "devDependencies": {
    "@types/node": "^10.12.12",
    "@types/request": "^2.48.1",
    "nodemon": "^1.18.7",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.2.2"
  }
